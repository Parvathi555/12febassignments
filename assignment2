a=int(input('Enter a number'))
b=int(input('Enter another number'))
if a==b:
    print('equal')
else:
    print('Not equal')
    if a>b:
        print('a is greater than b')
    else:
        print('a is smaller than b')
        
        
        
 output1:Enter a number3
         Enter another number4
         Not equal
         a is smaller than b
         
 output2:Enter a number4
         Enter another number4
         equal
         
 output3:Enter a number15
         Enter another number6
         Not equal
         a is greater than b
